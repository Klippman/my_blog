<div id="parent_sign_in_div">
    <div id="sign_in">
        <div id="megaman">
            <img src="/images/megaman4.jpg">
        </div>
        <p id="login_blurp">SEE WHAT'S NEW!</p>
        <div id="login">
            <%= form_tag(create_session_path, method: "post") do %> 
            <%= label_tag(:email)%>
            <%= text_field_tag(:email)%>
            <%= label_tag(:password)%>
            <%= password_field_tag(:password)%>
            <%= submit_tag("Login")%>
            <% end %>
            </hr>
        </div>
    </div>
</div>